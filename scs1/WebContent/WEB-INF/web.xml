<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoggedInServlet</servlet-name>
    <servlet-class>login.LoggedInServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoggedInServlet</servlet-name>
    <url-pattern>/loggedin</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>search.Search</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/Search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Search2</servlet-name>
    <servlet-class>search.Search2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Search2</servlet-name>
    <url-pattern>/Search2</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InsertServlet</servlet-name>
    <servlet-class>add.InsertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertServlet</servlet-name>
    <url-pattern>/save</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InsertedServlet</servlet-name>
    <servlet-class>add.InsertedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertedServlet</servlet-name>
    <url-pattern>/saved</url-pattern>
  </servlet-mapping>
  
  
    <servlet>
    <servlet-name>StudentInsert</servlet-name>
    <servlet-class>add2.StudentInsert</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StudentInsert</servlet-name>
    <url-pattern>/save1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StudentInserted</servlet-name>
    <servlet-class>add2.StudentInserted</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertedServlet</servlet-name>
    <url-pattern>/saved1</url-pattern>
  </servlet-mapping>
  
  
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>